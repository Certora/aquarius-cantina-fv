{
    "build_script": "../certora_build.py",
    "optimistic_loop": true,
    "loop_iter": "2",
    "process": "emv",
    "rule": [
        "apply_transfer_ownership_reverts_if_caller_not_admin",
        "apply_transfer_ownership_reverts_if_deadline_not_passed",
        "apply_transfer_ownership_reverts_if_deadline_zero",
        "apply_transfer_ownership_reverts_if_future_value_not_set",
        "apply_transfer_ownership_sets_deadline_to_zero",
        "apply_transfer_ownership_sets_adress_to_future_address",
        "apply_transfer_ownership_does_not_respect_deadline_if_role_not_set",
        "apply_transfer_ownership_returns_future_address",
        "revert_transfer_ownership_reverts_if_no_admin_role",
        "revert_transfer_ownership_reverts_if_not_admin"
    ]
}