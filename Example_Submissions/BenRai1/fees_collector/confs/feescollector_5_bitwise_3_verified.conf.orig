{
    "build_script": "../certora_build.py",
    "loop_iter": "5",
    "optimistic_loop": true,
    "process": "emv",
    "rule": [
        "revert_upgrade_reverts_if_no_admin_role",
        "revert_upgrade_sets_deadline_zero",
        "set_emergency_mode_reverts_if_not_emergency_admin",
        "set_emergency_mode_reverts_if_not_emergancy_admin",
        "set_emergency_mode_sets_emergency_mode",
        "only_emergency_admin_sets_emergency_mode",
        "set_emergency_mode_success",
        "transfer_ownership_must_respect_delay",
        "transfer_ownership_reverts_if_already_commited",
        "commit_transfer_ownership_reverts_not_admin_or_emergency_admin"
    ],
    "prover_version": "master",
    "precise_bitwise_ops": true
}