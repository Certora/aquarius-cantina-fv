{
    "build_script": "../certora_build.py",
    "optimistic_loop": true,
    "loop_iter": "2",
    "process": "emv",
    "rule": [
        "revert_transfer_ownership_sets_deadline_to_zero",
        "get_future_address_reverts_not_admin_or_emergency_admin",
        "get_future_address_reverts_if_not_scheduled_and_no_address",
        "get_future_address_reverts_if_role_not_admin_or_emergency_admin",
        "get_future_address_returns_future_address_if_scheduled",
        "get_future_address_returns_set_address_if_no_transfer_scheduled",
        "from_symbol_reverts_for_wrong_symbol",
        "require_pause_admin_or_owner_passes_for_admin"
    ]
}