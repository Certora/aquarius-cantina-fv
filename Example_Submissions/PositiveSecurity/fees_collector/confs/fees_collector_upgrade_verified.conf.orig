{
    "build_script": "../certora_build.py",
    "optimistic_loop": true,
    "loop_iter": "2",
    "process": "emv",
    "rule": [
        "init_admin_already_set_fails",
        "version_correct",
        "commit_upgrade_correct",
        "commit_upgrade_requires_admin",
        "commit_upgrade_another_action_active_fails",
        "apply_upgrade_correct",
        "apply_upgrade_requires_admin",
        "apply_upgrade_before_deadline_fails",
        "apply_upgrade_no_action_active_fails",
        "apply_upgrade_not_initialized_fails",
        "revert_upgrade_correct",
        "revert_upgrade_requires_admin",
        "set_emergency_mode_correct",
        "get_emergency_mode_correct"
    ]
}